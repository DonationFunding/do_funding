<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="order.model.Order">
	<insert id="InsertOrder">
		insert into do_order(o_num, o_mnum, o_qty, o_date) 
		values(seqoid.nextval, #{o_mnum}, #{o_qty}, sysdate)
	</insert>
	
	<select id="getMaxO_num" resultType="int">
		select max(o_num) from do_order
	</select>

<!-- 	
	<select id="">
		select od.odid, o.orderdate from (orders o inner JOIN members m on m.id like o.mid) 
		inner JOIN orderdetails od o.oid = od.oid where m.id='';
	</select>
 -->
 	<select id="OrderList" resultType="MyOrder">
 		select * from do_order where o_mnum = #{o_mnum} order by o_num desc
 	</select>
</mapper>

<!-- order.xml -->